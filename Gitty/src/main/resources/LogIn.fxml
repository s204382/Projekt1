<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<FlowPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="control.LogIn"
            prefHeight="400.0" prefWidth="600.0">
    <Label fx:id="label1">Enter Mail</Label>
    <Label fx:id="label2">Enter Code</Label>
    <TextField fx:id="fieldMail" prefWidth="200.0" />
    <TextField fx:id="fieldCode" prefWidth="200.0" />
    <Button fx:id="buttonLogin" onAction="#btnClick" text="press here to log in" />


</FlowPane>
